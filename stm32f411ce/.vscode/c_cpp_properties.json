{
    "configurations": [
        {
            "name": "WSL",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../libopencm3/include",
            ],
            "defines": ["_DEBUG", "UNICODE", "_UNICODE", "STM32F411xE", "USE_FULL_LL_DRIVER"],
            "cStandard": "gnu11",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "linux-gcc-arm",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerArgs": ["-DSTM32F411xE", "-mthumb", "-mcpu=cortex-m4"]
        },
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "~/STM32Cube/Repository/STM32Cube_FW_F1_V1.8.4/Drivers/CMSIS/Device/ST/STM32F1xx/Include",
                "~/STM32Cube/Repository/STM32Cube_FW_F1_V1.8.4/Drivers/CMSIS/Include"
            ],
            "defines": ["_DEBUG", "UNICODE", "_UNICODE", "STM32F103xB", "USE_FULL_LL_DRIVER"],
            "cStandard": "gnu11",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "macos-gcc-arm",
            "compilerPath": "/usr/local/bin/arm-none-eabi-gcc",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerArgs": ["-DSTM32F103xB", "-mthumb", "-mcpu=cortex-m3"]
        }
    ],
    "version": 4
}
